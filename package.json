{
  "private": true,
  "name": "MetroZoneGames",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --template src/template.html --prerenderUrls src/prerender-urls.json",
    "serve": "preact build --template src/template.html && preact serve",
    "dev": "preact watch --template src/template.html",
    "deploy": "preact build --template src/template.html --prerenderUrls src/prerender-urls.json && aws s3 sync build/ s3://spages.mobileposse.com/ && aws cloudfront create-invalidation --distribution-id E2SA8OWD3VYCAI --paths '/*'",
    "cache": "s3cmd --recursive modify --add-header=\"Cache-Control:public ,max-age= 31536000\" s3://spages.mobileposse.com",
    "aws-clear": "aws cloudfront create-invalidation --distribution-id E2SA8OWD3VYCAI --paths '/*'",
    "lint": "eslint src",
    "purge": "./purgeSpages.sh \"spages.mobileposse.com\" \"1d\"",
    "analyze": "preact build --analyze --template src/template.html",
    "light": "lighthouse https://spages.mobileposse.com/dash/2 --view"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "aws": "0.0.3-2",
    "cache": "^2.1.0",
    "clear": "^0.1.0",
    "eslint": "^6.4.0",
    "eslint-config-synacor": "^3.0.5",
    "eslint-plugin-react": "^7.14.3",
    "for-own": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lodash.tail": "^4.1.1",
    "mixin-object": "^3.0.0",
    "node-sass": "^4.12.0",
    "per-env": "^1.0.2",
    "preact-cli": "^2.2.1",
    "preact-render-spy": "^1.2.1",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^7.3.1",
    "type-check": "^0.3.2",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-dashboard": "^3.2.0",
    "wordwrap": "^1.0.0"
  },
  "dependencies": {
    "preact": "^10.0.0-rc.3",
    "preact-context": "^1.1.3",
    "preact-material-components": "^1.6.0",
    "preact-render-to-string": "^5.0.6",
    "preact-router": "^3.0.1",
    "react-ga": "^2.6.0",
    "react-gpt": "^2.0.1",
    "react-notify-toast": "^0.5.0",
    "react-particle-effect-button": "^1.0.1",
    "react-select": "^3.0.5",
    "react-star-rating-component": "^1.4.1"
  }
}
